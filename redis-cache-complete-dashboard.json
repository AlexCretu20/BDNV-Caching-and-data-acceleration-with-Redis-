{
  "title": "Redis Cache Performance - Dashboard",
  "tags": [
    "redis",
    "cache",
    "mongodb",
    "demo",
    "scenario1"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m",
      "15m"
    ]
  },
  "templating": {
    "list": [
      {
        "name": "scenario",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(cache_hit_rate_percent, scenario)",
        "multi": true,
        "includeAll": true,
        "allValue": ".*"
      },
      {
        "name": "pattern",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(cache_hits_total, pattern)",
        "multi": true,
        "includeAll": true,
        "allValue": ".*"
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "name": "Cache Cleared",
        "datasource": "Prometheus",
        "enable": true,
        "expr": "changes(redis_db_keys[1m]) < -100",
        "iconColor": "red",
        "tagKeys": "event"
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "id": 100,
      "title": "\ud83d\udcca Overview Metrics",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 24,
        "h": 1
      },
      "collapsed": false
    },
    {
      "id": 2,
      "title": "Total Requests",
      "type": "stat",
      "gridPos": {
        "x": 6,
        "y": 1,
        "w": 6,
        "h": 5
      },
      "targets": [
        {
          "expr": "sum(cache_hits_total{scenario=~\"$scenario\"}) + sum(cache_misses_total{scenario=~\"$scenario\"})",
          "legendFormat": "Total",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "blue"
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "value_and_name"
      }
    },
    {
      "id": 3,
      "title": "Hits vs Misses",
      "type": "stat",
      "gridPos": {
        "x": 12,
        "y": 1,
        "w": 6,
        "h": 5
      },
      "targets": [
        {
          "expr": "sum(cache_hits_total{scenario=~\"$scenario\"})",
          "legendFormat": "Hits",
          "refId": "A"
        },
        {
          "expr": "sum(cache_misses_total{scenario=~\"$scenario\"})",
          "legendFormat": "Misses",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Hits"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "green"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Misses"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none"
      }
    },
    {
      "id": 4,
      "title": "Cache Evictions",
      "type": "stat",
      "gridPos": {
        "x": 18,
        "y": 1,
        "w": 6,
        "h": 5
      },
      "targets": [
        {
          "expr": "sum(cache_evictions_total{scenario=~\"$scenario\"})",
          "legendFormat": "Evictions",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 100,
                "color": "yellow"
              },
              {
                "value": 500,
                "color": "red"
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area"
      }
    },
    {
      "type": "row",
      "id": 103,
      "title": "\ud83d\udcc9 Performance Evolution (MetricsVisualizer: plot_performance_over_time)",
      "gridPos": {
        "x": 0,
        "y": 24,
        "w": 24,
        "h": 1
      },
      "collapsed": false
    },
    {
      "id": 10,
      "title": "Request Volume (Cumulative)",
      "type": "timeseries",
      "gridPos": {
        "x": 0,
        "y": 31,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "expr": "sum(cache_hits_total{scenario=~\"$scenario\"}) + sum(cache_misses_total{scenario=~\"$scenario\"})",
          "legendFormat": "Total Requests",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 30,
            "gradientMode": "hue"
          },
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 11,
      "title": "Cache Evictions (LRU Policy)",
      "type": "timeseries",
      "gridPos": {
        "x": 12,
        "y": 31,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "expr": "sum(cache_evictions_total{scenario=~\"$scenario\"})",
          "legendFormat": "Evictions",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "bars",
            "lineWidth": 1,
            "fillOpacity": 80
          },
          "color": {
            "mode": "fixed",
            "fixedColor": "red"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    },
    {
      "type": "row",
      "id": 104,
      "title": "\ud83d\udd04 Pattern Usage (MetricsVisualizer: plot_pattern_comparison)",
      "gridPos": {
        "x": 0,
        "y": 37,
        "w": 24,
        "h": 1
      },
      "collapsed": false
    },
    {
      "id": 13,
      "title": "Cache Hit/Miss Ratio",
      "type": "piechart",
      "gridPos": {
        "x": 12,
        "y": 38,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum(cache_hits_total{scenario=~\"$scenario\"})",
          "legendFormat": "Hits",
          "refId": "A"
        },
        {
          "expr": "sum(cache_misses_total{scenario=~\"$scenario\"})",
          "legendFormat": "Misses",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Hits"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "green"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Misses"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "pieType": "donut",
        "displayLabels": [
          "name",
          "percent"
        ],
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true,
          "values": [
            "value",
            "percent"
          ]
        }
      }
    },
    {
      "type": "row",
      "id": 105,
      "title": "\ud83d\udd27 Redis Internals & Advanced Metrics",
      "gridPos": {
        "x": 0,
        "y": 46,
        "w": 24,
        "h": 1
      },
      "collapsed": false
    },
    {
      "id": 14,
      "title": "Redis Memory Usage",
      "type": "timeseries",
      "gridPos": {
        "x": 0,
        "y": 47,
        "w": 8,
        "h": 6
      },
      "targets": [
        {
          "expr": "redis_memory_used_bytes",
          "legendFormat": "Used Memory",
          "refId": "A"
        },
        {
          "expr": "redis_memory_max_bytes",
          "legendFormat": "Max Memory",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": {
            "fillOpacity": 10
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Max Memory"
            },
            "properties": [
              {
                "id": "custom.lineStyle",
                "value": {
                  "dash": [
                    10,
                    10
                  ],
                  "fill": "dash"
                }
              },
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "id": 15,
      "title": "Redis Commands per Second",
      "type": "timeseries",
      "gridPos": {
        "x": 8,
        "y": 47,
        "w": 8,
        "h": 6
      },
      "targets": [
        {
          "expr": "rate(redis_commands_processed_total[1m])",
          "legendFormat": "Commands/sec",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "fillOpacity": 20
          }
        }
      }
    },
    {
      "id": 16,
      "title": "Redis Total Keys",
      "type": "timeseries",
      "gridPos": {
        "x": 16,
        "y": 47,
        "w": 8,
        "h": 6
      },
      "targets": [
        {
          "expr": "sum(redis_db_keys)",
          "legendFormat": "Total Keys",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "fillOpacity": 30
          }
        }
      }
    },
    {
      "id": 18,
      "title": "Requests per Second",
      "type": "stat",
      "gridPos": {
        "x": 8,
        "y": 53,
        "w": 8,
        "h": 6
      },
      "targets": [
        {
          "expr": "sum(rate(cache_hits_total{scenario=~\"$scenario\"}[1m]) + rate(cache_misses_total{scenario=~\"$scenario\"}[1m]))",
          "legendFormat": "Req/sec",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "blue"
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
    "id": 19,
    "type": "timeseries",
    "title": "Latency Comparison: Cached vs Non-Cached (p95)",
    "datasource": "Prometheus",
    "gridPos": { "x": 0, "y": 60, "w": 24, "h": 8 },
    "targets": [
      {
        "refId": "A",
        "expr": "histogram_quantile(0.95, sum(rate(cache_read_latency_seconds_bucket[5m])) by (le))",
        "legendFormat": "Cache (Redis)"
      },
      {
        "refId": "B",
        "expr": "histogram_quantile(0.95, sum(rate(db_read_latency_seconds_bucket[5m])) by (le))",
        "legendFormat": "External API / DB"
      }
    ],
    "fieldConfig": {
      "defaults": { "unit": "s" }
    },
    "options": {
      "legend": { "displayMode": "list", "placement": "bottom" }
    }
  },
  {
    "id": 20,
    "type": "stat",
    "title": "Average Latency Reduction (External - Cache)",
    "datasource": "Prometheus",
    "gridPos": { "x": 0, "y": 68, "w": 8, "h": 6 },
    "targets": [
      {
        "refId": "A",
        "expr": "(\n  avg(rate(db_read_latency_seconds_sum[5m])) /\n  avg(rate(db_read_latency_seconds_count[5m]))\n)\n-\n(\n  avg(rate(cache_read_latency_seconds_sum[5m])) /\n  avg(rate(cache_read_latency_seconds_count[5m]))\n)"
      }
    ],
    "fieldConfig": {
      "defaults": {
        "unit": "s",
        "decimals": 4
      }
    },
    "options": {
      "colorMode": "value",
      "graphMode": "none",
      "textMode": "value_and_name"
    }
  },
  {
    "id": 21,
    "type": "timeseries",
    "title": "Eviction Rate (LRU) - evictions/sec",
    "datasource": "Prometheus",
    "gridPos": { "x": 8, "y": 68, "w": 8, "h": 6 },
    "targets": [
      {
        "refId": "A",
        "expr": "rate(cache_evictions_total[5m])",
        "legendFormat": "evictions/sec"
      }
    ],
    "fieldConfig": {
      "defaults": { "unit": "ops" }
    },
    "options": {
      "legend": { "displayMode": "list", "placement": "bottom" }
    }
  },
  {
    "id": 22,
    "type": "timeseries",
    "title": "Scalability Under Load: Requests per Second",
    "datasource": "Prometheus",
    "gridPos": { "x": 16, "y": 68, "w": 8, "h": 6 },
    "targets": [
      {
        "refId": "A",
        "expr": "sum(rate(cache_hits_total[1m]) + rate(cache_misses_total[1m]))",
        "legendFormat": "req/s"
      }
    ],
    "fieldConfig": {
      "defaults": { "unit": "reqps" }
    },
    "options": {
      "legend": { "displayMode": "list", "placement": "bottom" }
    }
  },
  {
    "id": 23,
    "type": "timeseries",
    "title": "Latency Under Load (p95) - Cache Read",
    "datasource": "Prometheus",
    "gridPos": { "x": 0, "y": 74, "w": 24, "h": 8 },
    "targets": [
      {
        "refId": "A",
        "expr": "histogram_quantile(0.95, sum(rate(cache_read_latency_seconds_bucket[1m])) by (le))",
        "legendFormat": "cache p95"
      }
    ],
    "fieldConfig": {
      "defaults": { "unit": "s" }
    },
    "options": {
      "legend": { "displayMode": "list", "placement": "bottom" }
    }
  }
  ]
}